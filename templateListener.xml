    <import>
        <templatelistener name="bookmarkGallery">
            <environment>user</environment>
            <templatename>image</templatename>
            <eventname>contentHeaderNavigation</eventname>
            <templatecode><![CDATA[{include file='__bookmarkGallery' application='gallery'}]]></templatecode>
        </templatelistener>

        <templatelistener name="userProfileBookmarkGallery">
            <environment>user</environment>
            <templatename>userProfileBookmark</templatename>
            <eventname>dropDownType</eventname>
            <templatecode><![CDATA[{include file='__userProfileBookmarkGallery' application='gallery'}]]></templatecode>
        </templatelistener>
    </import>
</data>
